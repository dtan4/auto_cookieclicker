{
    "manifest_version": 2,
    "name": "Auto CookieCliker",
    "version": "1.0",
    "description": "Automation tool for CookieClicker",
    "browser_action": {
        "default_icon": "img/icon_19.png"
    },
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "permissions": [
        "webRequest",
        "http://*/*",
        "https://*/*",
        "tabs"
    ]
}
