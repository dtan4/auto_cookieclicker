{
    "manifest_version": 2,
    "name": "Auto CookieCliker",
    "version": "1.0",
    "description": "Automation tool for CookieClicker",
    "icons": {
        "16": "img/icon_16.png",
        "32": "img/icon_32.png",
        "48": "img/icon_48.png",
        "128": "img/icon_128.png"
    },
    "browser_action": {
        "default_icon": "img/icon_19.png"
    },
    "content_scripts": [
        {
            "matches": [
                "http://orteil.dashnet.org/cookieclicker/",
                "http://cafe-capy.net/cookieclicker/"
            ],
            "js": ["jquery.min.js","script.js"],
            "run_at": "document_start"
        }
    ]
}
